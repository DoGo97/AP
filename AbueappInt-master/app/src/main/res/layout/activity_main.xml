<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0F52BA"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/Juana"
        android:layout_width="106dp"
        android:layout_height="116dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="50dp"
        android:onClick="cuadro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Familiares"
        app:srcCompat="@mipmap/b"
        tools:srcCompat="@tools:sample/avatars[4]" />

    <ImageView
        android:id="@+id/Juan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="5dp"
        android:onClick="cuadro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Familiares"
        app:srcCompat="@mipmap/a"
        tools:srcCompat="@mipmap/a" />

    <ImageView
        android:id="@+id/cat_o_miau"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="36dp"
        android:onClick="cuadro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Juant"
        app:srcCompat="@mipmap/c"
        tools:srcCompat="@tools:sample/avatars[3]" />

    <TextView
        android:id="@+id/Familiares"
        android:layout_width="400dp"
        android:layout_height="91dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="2dp"
        android:fontFamily="sans-serif"
        android:gravity="center|center_horizontal|center_vertical"
        android:text="Familiares"
        android:textAllCaps="true"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Maria"
        android:layout_width="110dp"
        android:layout_height="116dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="50dp"
        android:onClick="cuadro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Juanaa"
        app:srcCompat="@mipmap/d"
        tools:srcCompat="@tools:sample/avatars[1]" />

    <TextView
        android:id="@+id/Juant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="83dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="36dp"
        android:text="Juan"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/cat_o_miau"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Juan" />

    <TextView
        android:id="@+id/Juanaa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="83dp"
        android:text="Juana"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Juana" />

    <TextView
        android:id="@+id/catomiau"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="8dp"
        android:text="CatOMiau"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cat_o_miau" />

    <TextView
        android:id="@+id/Mariaq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="83dp"
        android:text="Maria"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Maria" />

    <TextView
        android:id="@+id/Emergencia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="177dp"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="175dp"
        android:gravity="center"
        android:text="Emergencia"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Maria" />

    <ImageButton
        android:id="@+id/emergenci"
        android:layout_width="300dp"
        android:layout_height="150dp"
        android:layout_marginStart="55dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="32dp"
        android:onClick="llama"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:color/holo_red_dark" />

    <ImageButton
        android:id="@+id/conf"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:onClick="conf"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_manage" />

    <ImageButton
        android:id="@+id/emergenc"
        android:layout_width="118dp"
        android:layout_height="92dp"
        android:layout_marginBottom="60dp"
        android:background="#00A50F0F"
        android:onClick="llama"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:drawable/ic_dialog_alert" />

    <Button
        android:id="@+id/llamada"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="32dp"
        android:enabled="false"
        android:onClick="llama"
        android:text="Llamar"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/mensaj"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginEnd="32dp"
        android:enabled="false"
        android:onClick="mensaje"
        android:text="Mensaje"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>